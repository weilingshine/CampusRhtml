@section scripts
{
    <script>
        (function () {
            document.getElementById("loader").style.display = "none";
            document.getElementById("myDiv").style.display = "block";
        })();

        @* 參加我要抽獎活動 *@
        $('#lnkLottery').click(function() {
            $.ajax({
                url: '@Url.Action("SignUpActivityAjax")',
                type: "post",
                data: {},
                dataType: "json",
                success: function(data) {
                    if (data !== null && data !== '') {
                        if (data.isOk) {
                            $('#lottery').modal('show');
                            $('#divStatus').html('<div id="prizecheck"><div class="btn over">已參加</div></div>');
                        } else {
                            alert(data.errMsg);
                        }
                    }
                },
                error: function(xhr, status, error) {
                    console.log(xhr);
                }
            });
        });

        @* 你已參加活動跳窗關閉時觸發 *@
        $('#certification').on('hidden.bs.modal',
            function() {
                location.reload();
            });
    </script>
}
